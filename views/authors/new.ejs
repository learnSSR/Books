<h2 class="page-header"> New Authors </h2>
<form action="/authors" method="POST">
	<div class="form-row">
		<div class="form-item">
		 <label >Name</label>
		 <input type="text" name="name" value="<%= author.name %>">
		</div>
	</div>
	 <div class="btn-row">
		 <a class="btn btn-primary" href="/authors/<%= author.id%>">Cancel</a>
		 <button class="btn btn-danger">Create</button>
	 </div>
</form>